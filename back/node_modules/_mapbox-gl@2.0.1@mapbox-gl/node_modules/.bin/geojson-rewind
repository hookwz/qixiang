#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_@mapbox_geojson-rewind@0.5.0@@mapbox/geojson-rewind/geojson-rewind" "$@"
  ret=$?
else 
  node  "$basedir/../../../_@mapbox_geojson-rewind@0.5.0@@mapbox/geojson-rewind/geojson-rewind" "$@"
  ret=$?
fi
exit $ret
